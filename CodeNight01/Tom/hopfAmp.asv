function amplitude=hopfAmp
%Find max amplitude for given alpha%
    count=0;
    
    epsilon=0.01;
    b=1;
    g=0;
    tSpan=500;
    initialValue=[0,-0.25];
    for a=linspace(0,1\sqrt(3))
        count=count+1;
        run=FNPlot(epsilon,a,b,g,tSpan,initialValue);
        maxamp(count)=max(max(run(:,2)));
        bifPlot(:,count)=[a,maxamp(count)];
    end
    plot(bifPlot)
end
